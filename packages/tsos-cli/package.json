{
    "name": "@brad-jones/tsos-cli",
    "version": "0.0.0-semantically-released",
    "description": "Provides a cli interface for the tsos-compiler.",
    "author": "Brad Jones <brad@bjc.id.au>",
    "repository": "brad-jones/tsos",
    "license": "MIT",
    "bin": {
        "tsos": "./dist/index.js"
    },
    "scripts": {
        "clean": "shx rm -rf ./dist && shx rm -rf ./.nyc_output",
        "build": "tsc -p tsconfig.build.json && shx cp -R ./dist/tsos-cli/**/* ./dist && shx rm -rf ./dist/tsos-* && shx chmod +x ./dist/index.js",
        "test": "echo 'Tests Coming Soon :)'",
        "cover": "echo 'Tests Coming Soon :)'",
        "upgrade-packages": "ncu --upgrade --upgradeAll --packageFile ./package.json --filter '/^(?!@brad-jones).*$/'"
    },
    "dependencies": {
        "@brad-jones/tsos-compiler": "0.0.0-semantically-released",
        "caporal": "cazgp/Caporal.js#8f353d593fe592c9fea51b4e565f1b3d747777a2",
        "change-case": "^3.0.1",
        "fs-extra": "^5.0.0",
        "inversify": "^4.9.0",
        "node-hook": "^1.0.0",
        "reflect-metadata": "^0.1.12",
        "replace-in-file": "^3.1.0",
        "require-glob": "^3.2.0",
        "source-map-support": "^0.5.2",
        "tslib": "^1.9.0",
        "xxhashjs": "^0.2.2"
    },
    "peerDependencies": {
        "typescript": "^2.6.2"
    },
    "devDependencies": {
        "@types/fs-extra": "^5.0.0",
        "@types/node": "^9.3.0",
        "@types/shelljs": "^0.7.7",
        "@types/source-map-support": "^0.4.0",
        "@types/xxhashjs": "^0.1.1",
        "alsatian": "^2.0.0",
        "npm-check-updates": "^2.14.0",
        "nyc": "^11.4.1",
        "shx": "^0.2.2",
        "tap-xunit": "^2.2.0",
        "typescript": "^2.6.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
