{
    "name": "@brad-jones/tsos-cli",
    "version": "0.0.0-semantically-released",
    "description": "Provides a cli interface for the tsos-compiler.",
    "author": "Brad Jones <brad@bjc.id.au>",
    "repository": "brad-jones/tsos",
    "license": "MIT",
    "bin": {
        "tsos": "./dist/index.js"
    },
    "scripts": {
        "clean": "shx rm -rf ./dist; shx rm -rf ./.nyc_output",
        "build": "tsc -p tsconfig.build.json; shx cp -R ./dist/tsos-cli/**/* ./dist; shx rm -rf ./dist/tsos-*; shx chmod +x ./dist/index.js",
        "test": "echo 'Tests Comming Soon :)';",
        "cover": "echo 'Tests Comming Soon :)';",
        "upgrade-packages": "ncu --upgrade --upgradeAll --packageFile ./package.json --filter '/^(?!@brad-jones).*$/'"
    },
    "dependencies": {
        "@brad-jones/tsos-compiler": "0.0.0-semantically-released",
        "caporal": "^0.9.0",
        "change-case": "^3.0.1",
        "inversify": "^4.6.0",
        "node-hook": "^1.0.0",
        "reflect-metadata": "^0.1.10",
        "replace-in-file": "^3.0.0",
        "require-glob": "^3.2.0",
        "shelljs": "^0.7.8",
        "source-map-support": "^0.5.0",
        "tslib": "^1.8.1",
        "xxhashjs": "^0.2.1"
    },
    "peerDependencies": {
        "typescript": "^2.6.2"
    },
    "devDependencies": {
        "@types/node": "^8.5.1",
        "@types/shelljs": "^0.7.7",
        "@types/source-map-support": "^0.4.0",
        "@types/xxhashjs": "^0.1.1",
        "alsatian": "^2.0.0",
        "npm-check-updates": "^2.13.0",
        "nyc": "^11.3.0",
        "shx": "^0.2.2",
        "tap-xunit": "^2.2.0",
        "typescript": "^2.6.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
