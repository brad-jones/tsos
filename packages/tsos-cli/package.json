{
    "name": "@brad-jones/tsos-cli",
    "version": "1.0.0",
    "description": "Provides a cli interface for the tsos-compiler.",
    "author": "Brad Jones <brad@bjc.id.au>",
    "license": "MIT",
    "bin": {
        "tsos": "./dist/index.js"
    },
    "scripts": {
        "clean": "shx rm -rf ./dist",
        "build": "tsc -p tsconfig.build.json",
        "watch": "nodemon -e ts,js,json,env -i ./dist -x 'bash -c' 'yarn run clean; yarn run build;'"
    },
    "dependencies": {
        "tslib": "^1.7.1",
        "shelljs": "^0.7.8",
        "xxhashjs": "^0.2.1",
        "inversify": "^4.3.0",
        "node-hook": "^1.0.0",
        "change-case": "^3.0.1",
        "require-glob": "^3.2.0",
        "reflect-metadata": "^0.1.10",
        "source-map-support": "^0.5.0",
        "replace-in-file": "^3.0.0-beta.2",
        "@brad-jones/tsos-compiler": "^1.0.0",
        "caporal": "https://github.com/mattallty/Caporal.js.git#master"
    },
    "peerDependencies": {
        "typescript": "^2.5.3"
    },
    "devDependencies": {
        "shx": "^0.2.2",
        "nodemon": "^1.12.1",
        "typescript": "^2.5.3",
        "@types/node": "^8.0.33",
        "@types/shelljs": "^0.7.4",
        "@types/xxhashjs": "^0.1.1",
        "@types/source-map-support": "^0.4.0"
    }
}
